<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
                    android:orientation="vertical"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">
    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:background="@drawable/login_shape"
        >
        <Button
            android:id="@+id/btn_clickback"
            android:layout_width="50dp"
            android:layout_height="wrap_content"
            android:paddingTop="20dp"
            android:paddingBottom="10dp"
            android:layout_marginLeft="10dp"
            android:background="@drawable/btn_click_change"
            />
        <TextView
            android:id="@id/textView_register"
            android:layout_width="fill_parent"
            android:layout_height="50dp"
            android:text="设置"
            android:textSize="25dp"
            android:drawablePadding="100dp"
            android:background="@drawable/login_shape"
            android:paddingTop="10dp"
            android:paddingBottom="10dp"
            android:textColor="#ffffff"
            android:layout_marginLeft="100dp"
            />
    </LinearLayout>

    <ScrollView
        android:layout_width="fill_parent"
        android:layout_height="fill_parent">
        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:orientation="vertical">
            <LinearLayout
                android:id="@+id/ll_carInfoManage"
                android:layout_width="fill_parent"
                android:layout_height="45dp"
                android:orientation="horizontal"
                android:layout_marginTop="13dp"
                android:layout_marginLeft="13dp"
                android:layout_marginRight="13dp"
                android:background="@drawable/setting_background"
                android:clickable="true"
                >
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="帐号管理"
                        android:textSize="18sp"
                        android:paddingLeft="80dp"
                        android:paddingTop="10dp"
                        />
                    <ImageView
                        android:layout_width="20dp"
                        android:layout_height="20dp"
                        android:src="@drawable/right_icon"
                        android:layout_marginLeft="155dp"
                        android:layout_marginTop="13dp"
                        />
            </LinearLayout>

            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="45dp"
                android:orientation="horizontal"
                android:layout_marginTop="13dp"
                android:layout_marginLeft="13dp"
                android:layout_marginRight="13dp"
                android:background="@drawable/setting_background"
                android:clickable="true"
                >
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="我的优惠券管理"
                    android:textSize="18sp"
                    android:paddingLeft="80dp"
                    android:paddingTop="10dp"
                    />
                <ImageView
                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:src="@drawable/right_icon"
                    android:layout_marginLeft="100dp"
                    android:layout_marginTop="13dp"
                    />
            </LinearLayout>
            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="45dp"
                android:orientation="horizontal"
                android:layout_marginTop="13dp"
                android:layout_marginLeft="13dp"
                android:layout_marginRight="13dp"
                android:background="@drawable/setting_background"
                android:clickable="true"
                >
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="修改密码"
                    android:textSize="18sp"
                    android:paddingLeft="80dp"
                    android:paddingTop="10dp"
                    />
                <ImageView
                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:src="@drawable/right_icon"
                    android:layout_marginLeft="155dp"
                    android:layout_marginTop="13dp"
                    />
            </LinearLayout>
            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="45dp"
                android:orientation="horizontal"
                android:layout_marginTop="13dp"
                android:layout_marginLeft="13dp"
                android:layout_marginRight="13dp"
                android:background="@drawable/setting_background"
                android:clickable="true"
                >
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="提醒状态设置"
                    android:textSize="18sp"
                    android:paddingLeft="80dp"
                    android:paddingTop="10dp"
                    />
                <ImageView
                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:src="@drawable/right_icon"
                    android:layout_marginLeft="120dp"
                    android:layout_marginTop="13dp"
                    />
            </LinearLayout>
            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="45dp"
                android:orientation="horizontal"
                android:layout_marginTop="13dp"
                android:layout_marginLeft="13dp"
                android:layout_marginRight="13dp"
                android:background="@drawable/setting_background"
                android:clickable="true"
                >
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="离线地图"
                    android:textSize="18sp"
                    android:paddingLeft="80dp"
                    android:paddingTop="10dp"
                    />
                <ImageView
                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:src="@drawable/right_icon"
                    android:layout_marginLeft="155dp"
                    android:layout_marginTop="13dp"
                    />
            </LinearLayout>
            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:layout_marginTop="13dp"
                android:layout_marginLeft="13dp"
                android:layout_marginRight="13dp"
                android:background="@drawable/setting_background"
                >
                <LinearLayout
                    android:id="@+id/ll_videoPlayer"
                    android:layout_width="fill_parent"
                    android:layout_height="45dp"
                    android:layout_margin="2dp"
                    android:background="@drawable/setting2"
                    android:clickable="true"
                    >
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="行车记录仪"
                        android:textSize="18sp"
                        android:paddingLeft="80dp"
                        android:paddingTop="10dp"
                        />
                    <ImageView
                        android:layout_width="20dp"
                        android:layout_height="20dp"
                        android:src="@drawable/right_icon"
                        android:layout_marginLeft="135dp"
                        android:layout_marginTop="13dp"
                        />
                </LinearLayout>
                <View
                    android:layout_width="fill_parent"
                    android:layout_height="1dp"
                    android:background="#EBEBEB" >
                </View>
                <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="45dp"
                    android:layout_margin="1dp"
                    android:background="@drawable/setting2"
                    android:clickable="true">
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="行车记录仪设置"
                        android:textSize="18sp"
                        android:paddingLeft="80dp"
                        android:paddingTop="10dp"
                        />
                    <ImageView
                        android:layout_width="20dp"
                        android:layout_height="20dp"
                        android:src="@drawable/right_icon"
                        android:layout_marginLeft="100dp"
                        android:layout_marginTop="13dp"
                        />
                </LinearLayout>
                <View
                    android:layout_width="fill_parent"
                    android:layout_height="1dp"
                    android:background="#EBEBEB" >
                </View>
                <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="45dp"
                    android:layout_marginLeft="3dp"
                    android:layout_marginRight="3dp"
                    android:layout_marginTop="1dp"
                    android:layout_marginBottom="4dp"
                    android:background="@drawable/setting2"
                    android:clickable="true">
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="视频文件管理"
                        android:textSize="18sp"
                        android:paddingLeft="80dp"
                        android:paddingTop="10dp"
                        />
                    <ImageView
                        android:layout_width="20dp"
                        android:layout_height="20dp"
                        android:src="@drawable/right_icon"
                        android:layout_marginLeft="118dp"
                        android:layout_marginTop="13dp"
                        />
                </LinearLayout>


            </LinearLayout>
            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:layout_marginTop="13dp"
                android:layout_marginLeft="13dp"
                android:layout_marginRight="13dp"
                android:background="@drawable/setting_background"
                >
                <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="45dp"
                    android:layout_margin="2dp"
                    android:background="@drawable/setting2"
                    android:clickable="true"
                    >
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="检查版本"
                        android:textSize="18sp"
                        android:paddingLeft="80dp"
                        android:paddingTop="10dp"
                        />
                    <ImageView
                        android:layout_width="20dp"
                        android:layout_height="20dp"
                        android:src="@drawable/right_icon"
                        android:layout_marginLeft="155dp"
                        android:layout_marginTop="13dp"
                        />
                </LinearLayout>
                <View
                    android:layout_width="fill_parent"
                    android:layout_height="1dp"
                    android:background="#EBEBEB" >
                </View>
                <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="45dp"
                    android:layout_marginLeft="3dp"
                    android:layout_marginRight="3dp"
                    android:layout_marginTop="1dp"
                    android:layout_marginBottom="4dp"
                    android:background="@drawable/setting2"
                    android:clickable="true">
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="关于"
                        android:textSize="18sp"
                        android:paddingLeft="80dp"
                        android:paddingTop="10dp"
                        />
                    <ImageView
                        android:layout_width="20dp"
                        android:layout_height="20dp"
                        android:src="@drawable/right_icon"
                        android:layout_marginLeft="190dp"
                        android:layout_marginTop="13dp"
                        />
                </LinearLayout>

            </LinearLayout>
            <Button
                android:id="@+id/btn_logout"
                android:background="@drawable/setting_logout"
                android:layout_marginTop="13dp"
                android:layout_marginLeft="13dp"
                android:layout_marginRight="13dp"
                android:layout_marginBottom="13dp"
                android:layout_width="fill_parent"
                android:layout_height="45dp"
                android:text="退出当前账户"
                android:textSize="18sp"
                android:textColor="@color/white"
                />

        </LinearLayout>
    </ScrollView>

</LinearLayout>
